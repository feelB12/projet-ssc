{% extends "profile/base.html.twig" %}

{% block metatitle %}
    Recherche  | Skateboard Social Club
{% endblock metatitle %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/details.css') }}">
    <link rel="stylesheet" href="{{ asset('css/burger.css') }}">
    <link rel="stylesheet" href="{{ asset('css/search.css') }}">
{% endblock stylesheets %}

{% block title %}
    searching result
{% endblock title %}

{% block main %}
    {% for club in clubs %}
        <div class="detail top20">
            <div class="img-card">
                <h2> {{ club.title }}</h2>
                <h3> {{ club.description }}</h3>
            </div>
        </div>
    {% endfor %}
{% endblock main %}